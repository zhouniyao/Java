package niming.parser;

import java.util.ArrayList;

import min3d.vos.Number3d;
import min3d.vos.Uv;

import android.content.res.Resources;

public class Parser {
	protected Resources resources;
	protected String resourceID;
	protected ArrayList<Number3d> vertices;//顶点坐标
	protected ArrayList<Uv> texCoords; //纹理坐标
	protected ArrayList<Number3d> normals;//顶点法向量
	protected ArrayList<face> faces;//面
	
	public Parser() {
		vertices = new ArrayList<Number3d>();
		texCoords = new ArrayList<Uv>();
		normals = new ArrayList<Number3d>();
	}
}
