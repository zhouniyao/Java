package com.niming;

public class ArrayTest {
	public static void main(String[] args) {
		int[] a = { 2, 3, 5, 7, 8};
		int[] b = new int[a.length + 1];
		
		for(int i= 0,j=0; i<a.length; i++){
			if(j != 2){
				b[j] = a[i];
				j++;
				
			}
			else(j == 2){
				b[j] = 100;
				continue;
			}
		}
		//Êä³öbÊý×é
		for (int i=0; i<b.length; i++) {
			System.out.println(b[i]);
		}
	}
}
