package com.niming;

public class ArrayTest {
	public static void main(String[] args) {
		int[] a = { 2, 3, 5, 7, 8};
		for (int i=0; i<a.length; i++) {
			System.out.println(a[i]);
		}
		int[] b = new int[a.length + 1];
		
		for(int i= 0,j=0; i<a.length; i++){
			b[j] = a[i];
			j++;
			if(j == 2){
				b[j] = 100;
				continue;
			}
		}
	}
}
